          $("#frmCrearModelo").hide();
          $("#frmAbrirModelo").hide();
          $("#divMenuPrincipal").hide();
          $("#divModelView").show();
        }else{
          alert(status);
        }
        $("#divProtector").hide();
      });
      
}

